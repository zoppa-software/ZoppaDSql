<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>ZoppaDSql</RootNamespace>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Title>Zoppa DSql</Title>
    <RepositoryUrl>https://github.com/zoppa-software/ZoppaDSql/tree/main</RepositoryUrl>
    <PackageProjectUrl>https://github.com/zoppa-software/ZoppaDSql/tree/main</PackageProjectUrl>
    <Authors>zoppa software</Authors>
    <Company></Company>
    <Copyright>Copyright Â© 2022 zoppa software</Copyright>
	  <VersionPrefix>1.0.7</VersionPrefix>
	  <PackageLicenseFile>License.txt</PackageLicenseFile>
	  <PackageReadmeFile>NugetReadme.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\NugetReadme.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Update="License.txt">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
